{
  "name": "drconnect-backend",
  "version": "1.0.0",
  "private": true,
  "description": "DrConnect - Doctor Consultation Platform Backend",
  "license": "MIT",
  "author": "DrConnect Team",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start": "node --max-old-space-size=2048 dist/main.js",
   "build": "NODE_OPTIONS=--max-old-space-size=2048 nest build",
    "copy-templates": "node copy-templates.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:seed": "ts-node ./src/cli.ts create:user",
    "setup-templates": "node ensure-templates.js && node copy-templates.js",
    "script:installment": "ts-node -r tsconfig-paths/register src/scripts/setup-installment-config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "latest",
    "@nestjs/microservices": "^10.4.20",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "latest",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^10.4.20",
    "axios": "^1.13.1",
    "bcryptjs": "latest",
    "class-transformer": "^0.5.1",
    "class-validator": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "crypto": "latest",
    "dotenv": "^16.3.1",
    "helmet": "^8.1.0",
    "mongoose": "latest",
    "nestjs-command": "^3.1.5",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "twilio": "latest"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/node": "^22",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
}
